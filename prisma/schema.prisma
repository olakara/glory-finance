datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Expenses {
    id        Int      @id @default(autoincrement())
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    refernceId  String @db.VarChar(255)
    title  String @db.VarChar(255)
    date DateTime
}